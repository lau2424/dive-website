
<div class="d-contenaire">
    <h2>Review this spot!</h2>
  <div class="d-box">
    <%= simple_form_for [spot, review] do |f| %>
      <%= f.input :content %>
      <%= f.label :rating %>
      <%= f.select(:rating, [0,1,2,3,4,5], {}, {:class => 'form-control'})%>
      <%= f.input :photo, as: :file %>
      <%= f.submit "Submit review", class: "btn-gps" %>
    <% end %>
  </div>
</div>
